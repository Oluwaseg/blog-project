    <section class="bg-white dark:bg-gray-900">
      <div class="py-8 px-4 mx-auto max-w-screen-xl lg:py-16 lg:px-6">
        <div class="mx-auto max-w-screen-sm text-center lg:mb-16 mb-8">
          <h2
            class="mb-4 text-3xl lg:text-4xl tracking-tight font-extrabold text-gray-900 dark:text-white"
          >
            Our Blog
          </h2>
          <p class="font-light text-gray-500 sm:text-xl dark:text-gray-400">
            We use an agile approach to test assumptions and connect with the
            needs of your audience early and often.
          </p>
        </div>
        <div class="grid gap-8 grid-cols-1 mt-8 md:mt-16 md:grid-cols-3">
          <% if (blogs.length<1) { %>
          <p class="font-light text-gray-500 sm:text-xl dark:text-gray-400">
            No posts yet, create a
            <a href="/blog/create" class="text-blue-600 font-bold">new one</a>
          </p>
          <% } %> <% blogs.sort((a, b) => new Date(b.createdAt) - new
          Date(a.createdAt)); %> <% blogs.forEach(blog => { %>
          <div class="container px-6 py-10 mx-auto">
            <div class="lg:flex">
              <div
                class="relative bg-gray-800 rounded-lg overflow-hidden group"
              >
                <div class="h-96">
                  <img
                    src="<%= blog.image || 'https://images.unsplash.com/photo-1515378960530-7c0da6231fb1?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1470&q=80' %>"
                    alt=""
                    class="w-full h-full object-cover transform group-hover:scale-105 transition duration-500"
                  />
                </div>
                <div
                  class="absolute bottom-0 left-0 right-0 bg-black bg-opacity-35 p-4"
                >
                  <div
                    class="flex justify-between items-center mb-2 text-gray-500"
                  >
                    <span
                      class="bg-primary-100 text-primary-800 text-xs font-medium inline-flex items-center px-2.5 py-0.5 rounded dark:bg-primary-200 dark:text-primary-800"
                    >
                      <svg
                        class="mr-1 w-3 h-3"
                        fill="currentColor"
                        viewBox="0 0 20 20"
                        xmlns="http://www.w3.org/2000/svg"
                      >
                        <path
                          fill-rule="evenodd"
                          d="M2 5a2 2 0 012-2h8a2 2 0 012 2v10a2 2 0 002 2H4a2 2 0 01-2-2V5zm3 1h6v4H5V6zm6 6H5v2h6v-2z"
                          clip-rule="evenodd"
                        ></path>
                        <path
                          d="M15 7h1a2 2 0 012 2v5.5a1.5 1.5 0 01-3 0V7z"
                        ></path>
                      </svg>
                      <%= blog.category %>
                    </span>
                    <span class="text-sm vertical-middle text-white"
                      ><%= blog.createdAt.toDateString() %></span
                    >
                  </div>
                  <a
                    href="#"
                    class="text-white font-bold text-lg mb-1 hover:underline inline-flex items-center"
                    ><%= blog.title %>
                    <svg
                      class="ml-2 w-4 h-4"
                      fill="currentColor"
                      viewBox="0 0 20 20"
                      xmlns="http://www.w3.org/2000/svg"
                    >
                      <path
                        fill-rule="evenodd"
                        d="M10.293 3.293a1 1 0 011.414 0l6 6a1 1 0 010 1.414l-6 6a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-4.293-4.293a1 1 0 010-1.414z"
                        clip-rule="evenodd"
                      ></path>
                    </svg>
                  </a>
                  <p class="text-white"><%= blog.description %></p>

                  <div class="space-x-4 mt-1 flex items-center justify-start">
                    <span class="text-sm text-white">
                      <svg
                        xmlns="http://www.w3.org/2000/svg"
                        class="w-6 h-6 text-white"
                        fill="currentColor"
                        viewBox="0 0 32 32"
                      >
                        <path
                          d="M 16 3 C 12.210938 3 8.765625 4.113281 6.21875 5.976563 C 3.667969 7.835938 2 10.507813 2 13.5 C 2 17.128906 4.472656 20.199219 8 22.050781 L 8 29 L 14.746094 23.9375 C 15.15625 23.96875 15.570313 24 16 24 C 19.789063 24 23.234375 22.886719 25.78125 21.027344 C 28.332031 19.164063 30 16.492188 30 13.5 C 30 10.507813 28.332031 7.835938 25.78125 5.976563 C 23.234375 4.113281 19.789063 3 16 3 Z M 16 5 C 19.390625 5 22.445313 6.015625 24.601563 7.589844 C 26.757813 9.164063 28 11.246094 28 13.5 C 28 15.753906 26.757813 17.835938 24.601563 19.410156 C 22.445313 20.984375 19.390625 22 16 22 C 15.507813 22 15.015625 21.972656 14.523438 21.925781 L 14.140625 21.894531 L 10 25 L 10 20.859375 L 9.421875 20.59375 C 6.070313 19.019531 4 16.386719 4 13.5 C 4 11.246094 5.242188 9.164063 7.398438 7.589844 C 9.554688 6.015625 12.609375 5 16 5 Z"
                        ></path>
                      </svg>
                    </span>
                    <span
                      class="text-white text-xs font-medium inline-flex items-center px-2.5 py-0.5"
                    >
                      <i
                        class="fa-sharp fa-solid fa-star"
                        style="color: #ffd43b"
                      ></i>
                      <i
                        class="fa-sharp fa-solid fa-star"
                        style="color: #ffd43b"
                      ></i>
                      <i
                        class="fa-sharp fa-regular fa-star-half-stroke"
                        style="color: #ffd43b"
                      ></i>
                    </span>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <% }); %>
        </div>
      </div>
    </section>

    <div class="category-section article-section">
      <h2 class="category-title">Article</h2>
      <% if (blogsByCategory.Article && blogsByCategory.Article.length > 0) { %>
      <% blogsByCategory.Article.forEach(blog => { %>
      <div class="blog-post article-post">
        <h3 class="blog-post-title"><%= blog.title %></h3>
        <p><%= blog.description %></p>
        <p>By: <%= blog.author.name %> (@<%= blog.author.username %>)</p>
      </div>
      <% }); %> <% } else { %>
      <p>No articles available.</p>
      <% } %>
    </div>

    <!-- News Section -->
    <div class="category-section news-section">
      <h2 class="category-title">News</h2>
      <% if (blogsByCategory.News && blogsByCategory.News.length > 0) { %> <%
      blogsByCategory.News.forEach(blog => { %>
      <div class="blog-post news-post">
        <h3 class="blog-post-title"><%= blog.title %></h3>
        <p><%= blog.description %></p>
        <p>By: <%= blog.author.name %> (@<%= blog.author.username %>)</p>
      </div>
      <% }); %> <% } else { %>
      <p>No news available.</p>
      <% } %>
    </div>

    <!-- Guide Section -->
    <div class="category-section guide-section">
      <h2 class="category-title">Guide</h2>
      <% if (blogsByCategory.Guide && blogsByCategory.Guide.length > 0) { %> <%
      blogsByCategory.Guide.forEach(blog => { %>
      <div class="blog-post guide-post">
        <h3 class="blog-post-title"><%= blog.title %></h3>
        <p><%= blog.description %></p>
        <p>By: <%= blog.author.name %> (@<%= blog.author.username %>)</p>
      </div>
      <% }); %> <% } else { %>
      <p>No guides available.</p>
      <% } %>
    </div>

