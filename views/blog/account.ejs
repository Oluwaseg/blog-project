    <script>
      var prevActiveTab = null; // Variable to store the previously active tab

      function openTab(evt, tabName) {
        var i, tabcontent, tablinks;

        tabcontent = document.getElementsByClassName("tabcontent");
        for (i = 0; i < tabcontent.length; i++) {
          tabcontent[i].style.display = "none";
        }

        tablinks = document.getElementsByClassName("tablinks");
        for (i = 0; i < tablinks.length; i++) {
          tablinks[i].classList.remove("active");
          tablinks[i].classList.remove("bg-purple-600", "text-white");
        }

        document.getElementById(tabName).style.display = "block";
        evt.currentTarget.classList.add("active");
        evt.currentTarget.classList.add("bg-purple-600", "text-white");

        // Remove styling from previously active tab
        if (prevActiveTab !== null && prevActiveTab !== evt.currentTarget) {
          prevActiveTab.classList.remove("active");
          prevActiveTab.classList.remove("bg-purple-400", "text-red");
        }

        prevActiveTab = evt.currentTarget; // Update previously active tab
      }

      // Remove the default click on page load
      document.querySelector(".tablinks").click();
    </script>

